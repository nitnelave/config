autocmd!
noremap <C-t> :tabopen<CR>

noremap ; :

noremap , ;

noremap S :tabnext<CR>
noremap H :tabprevious<CR>

noremap t j
noremap n k
noremap s l
noremap T 20j
noremap N 20k

noremap l n
noremap L N

noremap <C-h> :back<CR>
noremap <C-s> :forward<CR>

noremap <C-w> :tabclose<CR>

cnoremap vs :tabopen<Space>

set ds=duckduckgo

set gui=nonavigation

set complete=lhS

colorscheme vimPgray
