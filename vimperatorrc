autocmd!
noremap <C-t> :tabopen<CR>

noremap ; :

noremap , ;

noremap S :tabnext<CR>
noremap H :tabprevious<CR>

noremap t j
noremap n k
noremap s l
noremap T 20j
noremap N 20k

noremap l n
noremap L N

noremap <C-h> :back<CR>
noremap <C-s> :forward<CR>

noremap <C-n> <C-v><C-n>

noremap <C-w> <C-v><C-w>

cnoremap vs :tabopen<Space>

set ds=duckduckgo

set gui=tabs

set complete=lhS

autocmd LocationChange .* :fmapc
autocmd LocationChange mail\\.google\\.com/mail :fmap -depth 4 c / j k n p o u e x s r a # [ ] z ? gi gs gt gd ga gc

colorscheme darkness2
